#include <iostream>
#include <stdio.h>
#include <math.h>
 
int main()
{
    double mas[3][3];
	int sum1, sum2;
    printf("%s", "Введите значения через пробел:");
    for (int i = 0; i<3; i++)
	{
    for (int j = 0; j<3; j++)
	{
    printf("mas[%d][%d]=",i,j);
    scanf("%d", &mas[i][j]);
    }
	}
    sum1=(int)(mas[0][0])+(int)(mas[1][1])+(int)(mas[2][2]);
    sum2=(int)(mas[2][0])+(int)(mas[1][1])+(int)(mas[0][2]);
    printf("%d %d", sum1, sum2);
    return 0;
    }
    //первое
    
    #include <iostream>
#include <stdio.h>
#include <math.h>
 
int main()
{
    int mas[2][2];
	int sum1, sum2;
    printf("%s", "Введите значения через пробел:");
    for (int i = 0; i<2; i++)
	{
    for (int j = 0; j<2; j++)
	{
    printf("mas[%d][%d]=",i,j);
    scanf("%d", &mas[i][j]);
    }
	}
    sum1=mas[0][0]+mas[1][1];
    sum2=mas[1][0]+mas[1][1];
    printf("%d %d", sum1, sum2);
    return 0;
    }
    
    //второе
