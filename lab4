#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(int argc, char *argv[])
{ double a,b,c,e,f;
printf("enter 3 numbers:\n");
scanf("%d", &a);
scanf("%d", &b);
scanf("%d", &c);
int perimeter(double a, double b, double c)
	{
		double p;
		p=a+b+c;
		return p;
	}
int square(double a, double b, double c)
{
	double s,p;
	p=(a+b+c)/2;
	s=sqrt(p*(p-a)*(p-b)*(p-c));
	return s;
}
switch(a,b,c)
	{
	case (a>b+c): e = perimeter(double a, double b, double c), f = perimeter(double a, double b, double c)
	case (b>a+c):e = perimeter(double a, double b, double c), f = perimeter(double a, double b, double c)
	case (c>b+a):e = perimeter(double a, double b, double c), f = perimeter(double a, double b, double c)
	default: printf("does not satisfy the condition");
	}
	return 0;
	printf("%d %d", e,f)
} 
